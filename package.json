{
  "name": "built-with-tachyons",
  "version": "1.0.0",
  "description": "A showcase of sites built with tachyons.io",
  "private": true,
  "scripts": {
    "build": "nwb build-react-app",
    "clean": "nwb clean-app",
    "deploy": "npm run build && gh-pages -d dist",
    "start": "nwb serve-react-app",
    "test": "standard && nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "standard && nwb test --server",
    "capture": "babel-node capture",
    "add": "babel-node add"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "copy-paste": "^1.3.0",
    "gh-pages": "^0.11.0",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "nwb": "0.12.x",
    "pageres": "^4.1.2"
  },
  "author": "Lachlan Campbell <lachlan.campbell@icloud.com>",
  "license": "MIT",
  "repository": "https://github.com/lachlanjc/built-with-tachyons",
  "standard": {
    "ignore": [
      "data.js"
    ],
    "parser": "babel-eslint"
  }
}
